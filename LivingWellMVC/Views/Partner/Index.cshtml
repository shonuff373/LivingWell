@{
    ViewBag.Title = "Partner With Us";
}

<section class="nomargin nopadding"></section><!--Buffer for header-->
<!-- Main Image
============================================= -->

<section id="slider" class="boxed-slider swiper_wrapper nopadding clearfix">
    <div class="swiper_wrapper_med_height">

        <div class="swiper-container swiper-parent">
            <div class="swiper-wrapper">
                <div class="swiper-slide dark main-image-parter-with-us">
                    <div class="container clearfix">
                        <div class="slider-caption slider-caption-center">
                            <h2 data-caption-animate="fadeInUp">Partner With Us</h2>
                            <p data-caption-animate="fadeInUp" data-caption-delay="200">Side by side, we'll find a solution that's right for you!</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</section>

<section id="content">
    <div class="container red-divider"></div>


    <!-- Free Analysis
    ============================================= -->
    <div class="nopadding container clearfix">
        <div class="section-block topmargin bottommargin center">
            <div class="section-title">
                <h3 class="col_full bottompadding-lg">
                    Let's Get Started
                </h3>

            </div>
        </div>
        <div class="col_half nobottommargin">
            <form id="template-analysis-form" name="template-analysis-form" method="post" role="form" novalidate="novalidate" class="allpadding-sm analysis-form">

                <div class="form-process"></div>

                <div class="col_half">
                    <label for="template-analysis-fname" class="dark">First Name <small>*</small></label>
                    <input type="text" id="template-analysis-fname" name="template-analysis-fname" value="" class="sm-form-control required" aria-required="true">
                </div>

                <div class="col_half col_last">
                    <label for="template-analysis-lname" class="dark">Last Name <small>*</small></label>
                    <input type="text" id="template-analysis-lname" name="template-analysis-lname" value="" class="sm-form-control required" aria-required="true">
                </div>

                <div class="clear"></div>

                <div class="col_full">
                    <label for="template-analysis-community-name" class="dark">Community Name</label>
                    <input type="text" id="template-analysis-community-name" name="template-analysis-community-name" value="" class="sm-form-control">
                </div>

                <div class="col_half">
                    <label for="template-analysis-address-one" class="dark">Community Address One</label>
                    <input type="text" id="template-analysis-address-one" name="template-analysis-address-one" value="" class="sm-form-control">
                </div>

                <div class="col_one_fourth">
                    <label for="template-analysis-address-two" class="dark">Address Two</label>
                    <input type="text" name="template-analysis-address-two" id="template-analysis-address-two" value="" class="sm-form-control">
                </div>

                <div class="col_one_fourth col_last">
                    <label for="template-analysis-city" class="dark">City</label>
                    <input type="text" name="template-analysis-city" id="template-analysis-city" value="" class="sm-form-control">
                </div>

                <div class="col_one_sixth">
                    <label for="template-analysis-state" class="dark">State</label>
                    <input type="text" name="template-analysis-state" id="template-analysis-state" value="" class="sm-form-control">
                </div>

                <div class="col_one_sixth">
                    <label for="template-analysis-postal-code" class="dark">Zip</label>
                    <input type="text" name="template-analysis-postal-code" id="template-analysis-postal-code" value="" class="sm-form-control">
                </div>

                <div class="col_one_third">
                    <label for="template-analysis-phone" class="dark">Phone</label>
                    <input type="tel" name="template-analysis-phone" id="template-analysis-phone" value="" class="sm-form-control">
                </div>

                <div class="col_one_third col_last">
                    <label for="template-analysis-email" class="dark">Email <small>*</small></label>
                    <input type="email" name="template-analysis-email" id="template-analysis-email" value="" class="sm-form-control email required" aria-required="true">
                </div>

                <div class="col_full">
                    <label for="template-analysis-message" class="dark">Message</label>
                    <textarea type="email" name="template-analysis-message" id="template-analysis-message" value="" class="sm-form-control" rows="6"></textarea>
                </div>
                <div class="clear"></div>


                <div class="col_full">
                    <button onclick="analysisSubmissionClick(); return false;" class="button button-3d button-large btn-block nomargin" name="template-analysis-apply">Schedule Analysis</button>
                </div>

            </form>

        </div>


        <div class="col_half bottommargin-sm col_last">

            <p class="lead dark-blue-text" style="font-size:xx-large; text-align:justify">
                Living Well Rehab promotes a unique approach to care by using the latest advancements in rehabilitation and nursing care.
                Our solutions ensure that you will no longer need to concern yourself with rising staff costs,
                staffing coverage, or lost revenue.  Allow us to evolve your rehab department to excellence!
            </p>

        </div>

    </div>
</section>